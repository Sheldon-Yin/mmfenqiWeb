{	
  "manifest_version": 2,
  "name":"我的第一个扩展",
  "version":"1.0.0.0",
  "permissions": ["<all_urls>","storage","notifications"],
  "browser_action": {
  "default_icon":"icon128.png",
  "default_popup":"popup.html"
  },
  "background": {
	"page":"background.html"
  },
  "content_scripts": [ {		
		"js": [ "jquery-2.0.3.min.js","cs1.js"],
		"matches": [ "*://www.csdn.net/*","file://*" ]
	}],
	"icons": {		
		"128": "icon128.png"
	},
	"web_accessible_resources" : [  
	"icon128.png"  
	]
}